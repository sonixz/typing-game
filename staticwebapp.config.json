{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/words.json"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".html": "text/html"
  },
  "globalHeaders": {
    "cache-control": "public, max-age=3600"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
